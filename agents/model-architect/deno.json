{
  "imports": {
    "std/": "https://deno.land/std@0.220.1/",
    "js-pytorch": "./src/utils/mock-pytorch.ts"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window"],
    "strict": true,
    "types": ["./src/types/deno.d.ts"]
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-write --allow-read",
    "train": "deno run --allow-net --allow-env --allow-write --allow-read --config deno.json",
    "optimize:auto:image": "deno run --allow-net --allow-env --allow-write --allow-read --config deno.json examples/optimize-cnn.ts",
    "optimize:auto:text": "deno run --allow-net --allow-env --allow-write --allow-read --config deno.json examples/optimize-text.ts",
    "optimize:interactive:image": "deno run --allow-net --allow-env --allow-write --allow-read --config deno.json examples/interactive-cnn.ts",
    "optimize:interactive:text": "deno run --allow-net --allow-env --allow-write --allow-read --config deno.json examples/interactive-text.ts"
  }
}
